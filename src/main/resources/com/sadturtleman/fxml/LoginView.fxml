<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="146.0" prefWidth="161.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.sadturtleman.controller.LoginController">
   <children>
      <Label prefHeight="17.0" prefWidth="57.0" text="ID" />
      <TextField fx:id="Id" />
      <Label prefHeight="17.0" prefWidth="91.0" text="PASSWORD" />
      <PasswordField fx:id="Password" />
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="10.0">
         <children>
            <Button mnemonicParsing="false" onMouseClicked="#login" text="LOGIN" />
            <Button mnemonicParsing="false" onMouseClicked="#signUp" text="SIGN UP" />
         </children>
         <padding>
            <Insets bottom="20.0" />
         </padding>
      </HBox>
   </children>
</VBox>
